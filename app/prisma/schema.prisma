// Database config
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

// Tables
model Drone {
  id            String   @id @default(cuid())
  serialNumber  String   @unique
  name          String
  email         String
  phoneNumber   String
  distance      Float
  violationTime DateTime
}
